{
  "name": "getinzone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "luxon": "^1.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "1.1.4",
    "tailwindcss": "^0.5.2"
  },
  "scripts": {
    "build:css": "postcss src/style.css -o src/index.css",
    "watch:css": "postcss src/style.css -o src/index.css -w",
    "start": "npm run watch:css & NODE_PATH=src react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "postbuild": "cp -r build docs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
